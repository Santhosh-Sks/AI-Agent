]
# Export environment variables
export N8N_PORT=${N8N_PORT:-5678}
export N8N_PROTOCOL=${N8N_PROTOCOL:-https}
export N8N_HOST=${N8N_HOST:-ai-agent-n8n.onrender.com}
export WEBHOOK_URL=${WEBHOOK_URL:-https://ai-agent-n8n.onrender.com/}
export N8N_USER_FOLDER=${N8N_USER_FOLDER:-/data}
export N8N_BASIC_AUTH_ACTIVE=${N8N_BASIC_AUTH_ACTIVE:-true}
export N8N_BASIC_AUTH_USER=${N8N_BASIC_AUTH_USER:-admin}
export N8N_BASIC_AUTH_PASSWORD=${N8N_BASIC_AUTH_PASSWORD:-changeme}
export N8N_ENCRYPTION_KEY=${N8N_ENCRYPTION_KEY:-random_generated_key}
export GENERIC_TIMEZONE=${GENERIC_TIMEZONE:-Asia/Kolkata}

echo "âœ… Environment variables configured:"
echo " - N8N_HOST: $N8N_HOST"
echo " - WEBHOOK_URL: $WEBHOOK_URL"
echo " - N8N_PORT: $N8N_PORT"
echo " - N8N_USER_FOLDER: $N8N_USER_FOLDER"
echo " - N8N_BASIC_AUTH_USER: $N8N_BASIC_AUTH_USER"
echo " - GENERIC_TIMEZONE: $GENERIC_TIMEZONE"

# Run n8n
exec n8n
